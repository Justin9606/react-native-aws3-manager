{
  "name": "react-native-s3-manager",
  "version": "1.1.6",
  "description": "Pure JavaScript react native library for uploading, deleting, and retrieving from AWS S3",
  "author": {
    "name": "Tokhirjon"
  },
  "keywords": [
    "react",
    "react-native",
    "aws",
    "s3",
    "image",
    "upload",
    "delete",
    "retrieve",
    "ios",
    "android"
  ],
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.1658.0",
    "buffer": "^4.5.1",
    "crypto-js": "^3.3.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.64.0",
    "@types/xml2js": "^0.4.14",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Justin9606/react-native-s3-manager"
  },
  "bugs": {
    "url": "https://github.com/Justin9606/react-native-s3-manager/issues"
  },
  "license": "MIT"
}
